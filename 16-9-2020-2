using System;		
using System.Collections;

public class Program
{	
	int number = 1;
	int number2 = 5;
	
	public void Main()
	{
		Console.WriteLine("Выберите + или -");
		string mySelect = Console.ReadLine();
		int otvet = mySelect == "+" ? (number + number2):otvet = mySelect == "-" ? (number - number2):(0);
		
		Console.WriteLine(otvet);
	}
}
